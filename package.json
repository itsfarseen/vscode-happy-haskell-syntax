{
  "name": "happy-haskell-syntax",
  "displayName": "Happy Haskell Syntax",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "happy",
        "aliases": [
          "Happy",
          "happy"
        ],
        "extensions": [
          ".y"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "happy",
        "scopeName": "source.happy",
        "path": "./syntaxes/happy.tmLanguage.json",
        "embeddedLanguages": {
          "meta.embedded.block.haskell": "haskell"
        }
      }
    ]
  },
  "devDependencies": {
    "js-yaml": "^3.13.1"
  }
}
